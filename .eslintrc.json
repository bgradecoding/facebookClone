{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "parser": "babel-eslint",
    "plugins": ["react", "import"],
    "rules": {
      "no-console": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }